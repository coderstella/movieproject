<!-- Start Banner Area -->
<section class="banner-area organic-breadcrumb">
    <div class="container">
        <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
            <div class="col-first">
                <h1>@ViewData["Title"]</h1>
                <nav class="d-flex align-items-center">
                    <a href="/">Home <span><i class="fa fa-angle-double-right" aria-hidden="true"></i></span></a>
                    <span style="color: #fff;">@ViewData["Title"]</span>
                </nav>
            </div>
        </div>
    </div>
</section>
<!-- End Banner Area -->